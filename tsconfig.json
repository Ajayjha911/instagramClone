{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
